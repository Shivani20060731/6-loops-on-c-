#include <iostream>
using namespace std;
int main() {
    int num;
    cout << "Enter a number: ";
    cin >> num;
    while (num != 0) {
        int digit = num % 10;   // Get last digit
        cout << digit << " ";
        num = num / 10;
}

    return 0;
}

//output//
Enter a number: 6785
5 8 7 6 
