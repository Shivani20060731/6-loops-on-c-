#include <iostream>
using namespace std;

int main() {
    char letter = 'A';  

    for(int i = 1; i <= 4; i++) {           // 4 rows
        for(int j = 1; j <= i; j++) {       // i letters in each row
            cout << letter << " ";
            letter++;                       
        }
        cout << endl;
    }

    return 0;
}


/output/
A
B C
D E F
G H I J